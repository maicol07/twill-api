{
  "name": "area17/twill-api",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Patrick VÃ©zina",
      "email": "patrick@area17.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:area17/laravel-translatable.git",
      "canonical": false
    },
    {
      "type": "vcs",
      "url": "git@github.com:area17/laravel-nestedset.git",
      "canonical": false
    },
    {
      "type": "vcs",
      "url": "git@github.com:ifox/tags.git",
      "canonical": false
    }
  ],
  "require": {
    "php": "^8.0",
    "area17/twill": "dev-laravel-11-support",
    "illuminate/support": "^11.0.3",
    "laravel-json-api/laravel": "dev-laravel11",
    "spatie/laravel-package-tools": "^1.11"
  },
  "require-dev": {
    "fakerphp/faker": "^1.15",
    "friendsofphp/php-cs-fixer": "^3.0",
    "laravel-json-api/testing": "^3.0",
    "mockery/mockery": "^1.4",
    "nunomaduro/collision": "^8.1",
    "orchestra/testbench": "^9.0",
    "phpunit/phpunit": "^11.0",
    "symfony/yaml": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "A17\\Twill\\API\\": "src"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php ./vendor/bin/testbench package:discover --ansi"
    ],
    "test": "./vendor/bin/testbench package:test"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "A17\\Twill\\API\\ServiceProvider"
      ],
      "aliases": {
        "TwillAPI": "A17\\Twill\\API\\Facade"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
